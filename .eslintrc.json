{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "react/jsx-filename-extension": [1, { "extensions": [".tsx", ".ts", ".js"] }],
        "comma-dangle": 0,
        "arrow-body-style": 0,
        "react/self-closing-comp": 0,
        "object-curly-newline": "off",
        "eol-last": 0
      }
}
